{
  "name": "neutralino-svelte-ts-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-svelte": "rollup -c",
    "dev-svelte": "rollup -c -w",
    "start-svelte": "sirv ressources --single --ignores --no-clear",
    "dev": "set USING_NEU=true && rollup -c -w",
    "build": "npm run build-svelte && neu build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  }
}
